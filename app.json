{
    "expo": {
        "name": "reactNativeApp",
        "slug": "reactNativeApp",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./app/assets/icon.png",
        "splash": {
            "image": "./app/assets/logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "web": {
            "favicon": "./app/assets/favicon.png"
        }
    }
}
